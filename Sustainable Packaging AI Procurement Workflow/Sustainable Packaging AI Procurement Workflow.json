{
  "name": "AI-Powered Sustainable Packaging Recommendation and Supplier Notification System",
  "nodes": [
    {
      "parameters": {
        "url": "https://api.packaginganalysis.example.com/analyze",
        "method": "POST",
        "jsonParameters": true,
        "options": {},
        "bodyParametersJson": "={{ { productId: $json[\"productId\"], packagingDetails: $json[\"packagingDetails\"] } }}",
        "headerParametersUi": {
          "parameter": [
            {
              "name": "Authorization",
              "value": "Bearer YOUR_PACKAGING_ANALYSIS_API_KEY"
            },
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ]
        }
      },
      "name": "Packaging Analysis API",
      "type": "httpRequest",
      "typeVersion": 1,
      "position": [
        250,
        300
      ]
    },
    {
      "parameters": {
        "model": "text-davinci-003",
        "prompt": "Analyze the following packaging analysis details and suggest eco-friendly, sustainable packaging alternatives along with benefits:\n\nPackaging Analysis: {{ $json[\"analysisResult\"] }}",
        "maxTokens": 500,
        "temperature": 0.7
      },
      "name": "AI Recommendation",
      "type": "openai",
      "typeVersion": 1,
      "position": [
        500,
        300
      ]
    },
    {
      "parameters": {
        "url": "https://api.suppliermanagement.example.com/orders",
        "method": "POST",
        "jsonParameters": true,
        "bodyParametersJson": "={{ { supplierId: $json[\"preferredSupplierId\"], productId: $json[\"productId\"], recommendedPackaging: $json[\"recommendation\"], quantity: $json[\"orderQuantity\"] } }}",
        "headerParametersUi": {
          "parameter": [
            {
              "name": "Authorization",
              "value": "Bearer YOUR_SUPPLIER_MANAGEMENT_API_KEY"
            },
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ]
        }
      },
      "name": "Supplier Order API",
      "type": "httpRequest",
      "typeVersion": 1,
      "position": [
        750,
        450
      ]
    },
    {
      "parameters": {
        "fromEmail": "no-reply@yourcompany.com",
        "toEmail": "={{ $json[\"supplierEmail\"] }}",
        "subject": "New Sustainable Packaging Order Request for Product {{ $json[\"productId\"] }}",
        "text": "Dear Supplier,\n\nWe have placed a new order for the following sustainable packaging solution:\n\n{{ $json[\"recommendation\"] }}\n\nOrder Quantity: {{ $json[\"orderQuantity\"] }}\n\nPlease confirm receipt and provide any updates.\n\nBest regards,\nSustainability Team"
      },
      "name": "Email Notification",
      "type": "emailSend",
      "typeVersion": 1,
      "position": [
        1000,
        450
      ]
    },
    {
      "parameters": {
        "functionCode": "return {\n  preferredSupplierId: \"supplier_123\",\n  supplierEmail: \"contact@sustainablesupplier.com\",\n  orderQuantity: 100\n};"
      },
      "name": "Set Supplier Details",
      "type": "function",
      "typeVersion": 1,
      "position": [
        750,
        300
      ]
    }
  ],
  "connections": {
    "Packaging Analysis API": {
      "main": [
        [
          {
            "node": "AI Recommendation",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "AI Recommendation": {
      "main": [
        [
          {
            "node": "Set Supplier Details",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Set Supplier Details": {
      "main": [
        [
          {
            "node": "Supplier Order API",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Supplier Order API": {
      "main": [
        [
          {
            "node": "Email Notification",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "active": false,
  "settings": {},
  "id": "1"
}