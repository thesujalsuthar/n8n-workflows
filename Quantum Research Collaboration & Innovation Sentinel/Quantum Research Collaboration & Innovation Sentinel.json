      "type": "n8n-nodes-base.mongodb",
      "typeVersion": 1,
      "position": [
        450,
        500
      ],
      "credentials": {
        "mongoDb": "Your MongoDB Credential"
      }
    },
    {
      "parameters": {
        "functionCode": "const trendsKeywords = ['quantum machine learning', 'error correction', 'quantum algorithms', 'quantum supremacy', 'entanglement', 'quantum cryptography'];\n\nconst papers = items[0].json;\n\nconst emergingTrends = trendsKeywords.filter(keyword =>\n  papers.some(paper => paper.title && paper.title.toLowerCase().includes(keyword))\n);\n\nreturn [{ json: { emergingTrends } }];"
      },
      "name": "Detect Emerging Trends",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [
        650,
        500
      ]
    },
    {
      "parameters": {
        "text": "Emerging Quantum Computing Trends Detected:\n{{$json[\"emergingTrends\"].join(', ') || 'No significant trends at the moment.'}}"
      },
      "name": "Prepare Notification Message",
      "type": "n8n-nodes-base.set",
      "typeVersion": 1,
      "position": [
        850,
        500
      ]
    },
    {
