      "parameters": {
        "functionCode": "const papers = items[0].json.items || items[0].json.entries || [];\n\nconst collaborations = [];\n\npapers.forEach(paper => {\n  if (paper.title && paper.title.toLowerCase().includes('collaboration')) {\n    collaborations.push(paper);\n  }\n});\n\nreturn collaborations.map(c => ({ json: c }));"
      },
      "name": "Filter Collaboration Opportunities",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [
        650,
        300
      ]
    },
    {
      "parameters": {
        "operation": "upsert",
        "collection": "collaborations",
        "data": "={{JSON.stringify($json)}}",
        "options": {
          "upsert": true
        },
        "key": "link"
      },
      "name": "Store Collaboration Opportunities",
      "type": "n8n-nodes-base.mongodb",
      "typeVersion": 1,
      "position": [
        850,
