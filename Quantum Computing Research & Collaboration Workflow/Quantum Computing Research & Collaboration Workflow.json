        "responseFormat": "json"
      },
      "name": "Fetch Collaboration Opportunities",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "credentials": {
        "linkedinApi": {
          "id": "1",
          "name": "LinkedIn API"
        }
