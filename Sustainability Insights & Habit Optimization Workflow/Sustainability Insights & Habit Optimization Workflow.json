      "type": "n8n-nodes-base.merge",
      "typeVersion": 1,
      "position": [
        400,
        700
      ]
    },
    {
      "parameters": {
        "functionCode": "const userData = items[0].json;\nconst aiTips = items[1].json.choices[0].message.content;\nconst communityData = items[2].json;\nconst socialFeeds = items[3].json.data ? items[3].json.data : [];\n\nreturn [{\n  json: {\n    dailyUserData: userData,\n    personalizedTips: aiTips,\n    communityImpact: communityData,\n    socialFeeds: socialFeeds\n  }\n}];"
      },
      "name": "Combine All Data",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [
        700,
        500
      ]
    },
    {
