{
  "name": "AI-Powered Personalized Sustainability Journey Planner and Habit Optimizer",
  "nodes": [
    {
      "parameters": {
        "authentication": "none",
        "requestMethod": "POST",
        "url": "https://api.openai.com/v1/chat/completions",
        "options": {},
        "bodyParametersUi": {
          "parameter": [
            {
              "name": "model",
              "value": "gpt-4"
            },
            {
              "name": "messages",
              "value": [
                {
                  "role": "system",
                  "content": "You are an AI assistant that helps create a personalized sustainability journey plan with tips, habit strategies, and progress tracking."
                },
                {
                  "role": "user",
                  "content": "Create a personalized sustainability journey plan considering user's current habits, preferences, and goals. Include actionable tips, habit formation techniques, environmental impact assessments, and community engagement suggestions."
                }
              ]
            },
            {
              "name": "temperature",
              "value": 0.7
            }
          ]
        },
        "headerParametersUi": {
          "parameter": [
            {
              "name": "Authorization",
              "value": "Bearer {{$env.OPENAI_API_KEY}}"
            },
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ]
        }
      },
      "name": "Generate Sustainability Plan",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [
        250,
        300
      ]
    },
    {
      "parameters": {
        "functionCode": "const planText = JSON.parse(items[0].json.body).choices[0].message.content;\nreturn [{ json: { planText } }];"
      },
      "name": "Parse AI Response",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [
        500,
        300
      ]
    },
    {
      "parameters": {
        "path": "user_habits",
        "options": {},
        "data": "={{ $json.planText }}",
        "dataProperty": "plan",
        "resource": "database",
        "operation": "update"
      },
      "name": "Store Plan in DB",
      "type": "n8n-nodes-base.firebase",
      "typeVersion": 1,
      "position": [
        750,
        300
      ],
      "credentials": {
